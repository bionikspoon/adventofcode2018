{
  "defaultSeverity": "warn",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "curly": [true, "ignore-same-line"],
    "object-literal-sort-keys": false,
    "radix": false,
    "max-classes-per-file": false,
    "variable-name": [
      true,
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}
